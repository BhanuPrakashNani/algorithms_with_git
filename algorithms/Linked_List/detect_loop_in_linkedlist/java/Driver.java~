public class Driver {

		public static void main(String[]args) {
			LinkedList l = new LinkedList();	//create object of the class Linkedlist
			l.insertfirst(60);
	        l.insertfirst(40);
	        l.insertfirst(30);
		    l.insertfirst(20);
		    l.insertfirst(10);
		    System.out.println("The data in the linked list");
		    l.printlist();
		    l.deleteAtPos(5);
		    l.deleteAtPos(3);
		    System.out.println("The list after the positions are deleted");
		    l.printlist();
		    l.isEmpty();
		    l.deleteData(10);
		    System.out.println("The list after deleting 10");
		    l.printlist();
		    l.isLast(40);
		    l.sortedInsertion(25);
		    l.sortedInsertion(30);
		    System.out.println("The list after the insertion");
		    l.printlist();
		    System.out.println("The data in an even indices");
		    l.evenprint();
		    System.out.println(l.num);
		    l.returnData(4);
		    l.middleElement(l.num);
		    l.sortedInsertion(30);
		    System.out.println("The actual size of the list = "+l.num);
		    l.printlist();
		    l.removeDuplicate();
		    System.out.println("The new Size after duplicates are removed = "+l.num);
		    l.printlist();
		    
		}
	}
